.sidejob-salon-main
  .sidejob-bg-opacity
    .sidejob-salon-page-header
      副業
    .sidejob-salon-profile-header
      .sidejob-salon-profile
        - @admins.each do |admin|
          .image
            = image_tag admin.image.url, alt: "NoImage"
          %br
          【サロン名】
          = admin.name
          %br
          【カテゴリー】
          = admin.category
          %br
          【所在地】
          = admin.birth_place
          %br
          = link_to "=詳細=", admin_path(admin), method: :get
          %br
          %br
          %br